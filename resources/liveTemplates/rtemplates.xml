<templateSet group="R">

    <template name="dd" value="ddply($DATA_FRAME$, .($SPLIT_VARS$), $FUN$)" description="ein test template"
              toReformat="false" toShortenFQNames="true">
        <variable name="DATA_FRAME" expression="" defaultValue="" alwaysStopAt="true"/>
